    "# PyPDF2 - Convert all PDFs to plain text\n",
    "current_time = millis()\n",
    "data = []\n",
    "\n",
    "for paper in papers:\n",
    "  data.append(pypdf2_parser(paper, 3))\n",
    "\n",
    "df_all_papers = pd.DataFrame.from_dict(data) \n",
    "\n",
    "df_all_papers_ordered_pypdf2 = reorder_columns(df_all_papers)\n",
    "\n",
